{
  "name": "stratego.js",
  "version": "1.0.0",
  "description": "A multiplayer webgame based on the board game 'Stratego', made with Node.js and Socket.io.",
  "main": "stratego.js",
  "scripts": {
    "start": "sass ./public/scss/style.scss:./public/css/style.css ./public/scss/game.scss:./public/css/game.css && node server/server.js",
    "dev": "nodemon server/server.js & sass --watch ./public/scss/style.scss:./public/css/style.css ./public/scss/game.scss:./public/css/game.css",
    "compile-sass": "sass ./public/scss/style.scss:./public/css/style.css ./public/scss/game.scss:./public/css/game.css",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelderoeck/stratego.git"
  },
  "author": "Axel De Roeck",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axelderoeck/stratego/issues"
  },
  "homepage": "https://github.com/axelderoeck/stratego#readme",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "sass": "^1.50.1"
  }
}
